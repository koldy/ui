{
	"name": "koldy-ui",
	"version": "0.1.9",
	"description": "Koldy UI - React UI component library",
	"main": "lib/index.js",
	"module": "esm/index.js",
	"scripts": {
		"test": "jest --silent",
		"dev": "webpack-dev-server --mode development",
		"build": "npm run build:esm && npm run build:cjs",
		"build:esm": "babel --delete-dir-on-start --env-name esm -d esm/ src/ --ignore ./src/docs.js --ignore ./src/**/*.test.js --ignore ./src/**/*.docs.js",
		"build:cjs": "babel --delete-dir-on-start --env-name cjs -d lib/ src/ --ignore ./src/docs.js --ignore ./src/**/*.test.js --ignore ./src/**/*.docs.js",
		"prepublishOnly": "npm run test && npm run build"
	},
	"author": "Vlatko Koudela",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/koldy/ui/issues"
	},
	"homepage": "https://koldy.io",
	"repository": {
		"type": "git",
		"url": "https://github.com/koldy/ui.git"
	},
	"peerDependencies": {
		"core-js": "^3.4.1",
		"prop-types": "^15.7.2",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"styled-components": "^4.4.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.7.0",
		"@babel/core": "^7.7.2",
		"@babel/plugin-proposal-class-properties": "^7.7.0",
		"@babel/plugin-proposal-throw-expressions": "^7.2.0",
		"@babel/preset-env": "^7.7.1",
		"@babel/preset-react": "^7.7.0",
		"acorn": "^7.1.0",
		"babel-loader": "^8.0.6",
		"babel-plugin-prismjs": "^1.1.1",
		"babel-plugin-styled-components": "^1.10.6",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"core-js": "^3.4.1",
		"css-loader": "^3.2.0",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.15.1",
		"jest": "^24.9.0",
		"prismjs": "^1.17.1",
		"prop-types": "^15.7.2",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"react-router-dom": "^5.1.2",
		"react-test-renderer": "^16.11.0",
		"styled-components": "^4.4.1",
		"webpack": "^4.41.2",
		"webpack-dev-server": "^3.9.0"
	},
	"files": [
		"lib/*",
		"esm/*"
	],
	"dependencies": {
		"popper.js": "^1.16.0"
	}
}
