{
	"name": "koldy-ui",
	"version": "0.3.0-beta.5",
	"description": "Koldy UI - React UI component library",
	"main": "lib/index.js",
	"module": "esm/index.js",
	"scripts": {
		"test": "jest --silent",
		"dev": "webpack-dev-server --mode development",
		"build": "npm run build:esm && npm run build:cjs",
		"build:esm": "babel --delete-dir-on-start --env-name esm -d esm/ src/ --ignore \"./src/docs.js\",\"./src/**/*.test.js\",\"./src/**/*.docs.js\"",
		"build:cjs": "babel --delete-dir-on-start --env-name cjs -d lib/ src/ --ignore \"./src/docs.js\",\"./src/**/*.test.js\",\"./src/**/*.docs.js\"",
		"prepublishOnly": "npm run test && npm run build",
		"lint": "eslint ./src --ignore-pattern *.docs.js --ignore-pattern *.test.js"
	},
	"author": "Vlatko Koudela",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/koldy/ui/issues"
	},
	"homepage": "https://koldy.io",
	"repository": {
		"type": "git",
		"url": "https://github.com/koldy/ui.git"
	},
	"peerDependencies": {
		"core-js": "^3.6.4",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"styled-components": "^5.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.8.4",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-throw-expressions": "^7.8.3",
		"@babel/polyfill": "^7.8.3",
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-react": "^7.8.3",
		"acorn": "^7.1.0",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"babel-plugin-prismjs": "^2.0.1",
		"babel-plugin-require-context-hook": "^1.0.0",
		"babel-plugin-styled-components": "^1.10.7",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"core-js": "^3.6.4",
		"css-loader": "^3.4.2",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.18.2",
		"eslint-plugin-react-hooks": "^2.3.0",
		"jest": "^25.1.0",
		"prettier": "^1.19.1",
		"prismjs": "^1.19.0",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-router-dom": "^5.1.2",
		"react-test-renderer": "^16.12.0",
		"styled-components": "^5.0.0",
		"webpack": "^4.41.5",
		"webpack-dev-server": "^3.10.2"
	},
	"files": [
		"lib/*",
		"esm/*"
	],
	"dependencies": {
		"popper.js": "^1.16.1"
	}
}
